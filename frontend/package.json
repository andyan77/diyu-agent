{
  "name": "diyu-frontend",
  "private": true,
  "bin": {
    "axe-core-cli": "./scripts/axe-core-cli"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "typecheck": "turbo run typecheck",
    "storybook": "turbo run storybook",
    "openapi:generate": "openapi-typescript openapi/openapi.yaml -o packages/api-client/src/generated/schema.d.ts",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.11.1",
    "@axe-core/playwright": "^4.11.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.58.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "turbo": "^2.3.0",
    "typescript": "^5.7.0"
  },
  "pnpm": {
    "overrides": {
      "minimatch@<10.2.1": ">=10.2.1"
    }
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}
